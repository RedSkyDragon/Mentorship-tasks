@using Microsoft.AspNetCore.Authentication
@{
    ViewData["Title"] = "About";
    var id_token = await Context.GetTokenAsync("id_token");
}
<h2>@ViewData["Title"]</h2>
<div>
    <dl>
        @foreach (var claim in User.Claims)
        {
            <dt>@claim.Type</dt>
            <dd>@claim.Value</dd>
        }
    </dl>
</div>